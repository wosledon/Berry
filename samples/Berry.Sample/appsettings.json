{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "Database": {
    "Provider": "Sqlite"
  },
  "ConnectionStrings": {
    "Default": "Data Source=App_Data/berry_sample.db"
  },
  "Caching": {
    "Provider": "Memory",
    "DefaultTtlSeconds": 300
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "Dev_Replace_This_With_A_Long_Random_Key_1234567890",
    "Issuer": "berry.sample",
    "Audience": "berry.clients",
    "ExpiresMinutes": 240
  },
  "Seed": {
    "TenantId": "public",
    "AdminUser": "admin",
    "AdminPassword": "ChangeMe123!"
  }
}
