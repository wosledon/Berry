{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    }
  },
  "Database": {
    "Provider": "Sqlite"
  },
  "ConnectionStrings": {
    "Default": "Data Source=App_Data/berry_sample.db"
  },
  "Caching": {
    "Provider": "Memory",
    "DefaultTtlSeconds": 300
  },
  "AllowedHosts": "*"
}
